  "scripts": {
    "ng": "ng",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve --host 0.0.0.0 --poll 2000",
    "build:ditectrev:staging": "ng build --project=ditectrev",
    "build:ditectrev:production": "ng build --project=ditectrev --prod",
    "build:ditectrev:ssr:staging": "npm run build:ditectrev:staging && ng run ditectrev:server",
    "build:ditectrev:ssr:production": "npm run build:ditectrev:production && ng run ditectrev:server:production",
    "build-and-serve:ditectrev:ssr:staging": "npm run build:ditectrev:ssr:staging && npm run serve:ditectrev:ssr",
    "build-and-serve:ditectrev:ssr:production": "npm run build:ditectrev:ssr:production && npm run serve:ditectrev:ssr",
    "build-and-serve:ditectrev:ssr:staging:firebase": "npm run build:ditectrev:ssr:staging && npm run root:starting:index:firebase && npm run serve:ditectrev:ssr:firebase",
    "build-and-serve:ditectrev:ssr:production:firebase": "npm run build:ditectrev:ssr:production && npm run root:starting:index:firebase && npm run serve:ditectrev:ssr:firebase",
    "lint": "nx workspace-lint && ng lint",
    "test": "ng test || exit 0",
    "e2e": "ng e2e",
    "predeploy": "firebase use --token $FIREBASE_DEPLOY_KEY $CI_ENVIRONMENT_SLUG",
    "deploy": "firebase deploy --token $FIREBASE_DEPLOY_KEY",
    "deploy:cloud_functions": "cd functions && firebase deploy --token $FIREBASE_DEPLOY_KEY",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:ditectrev:ssr": "node functions/dist/apps/ditectrev-server/main.js",
    "serve:ditectrev:ssr:firebase": "firebase serve",
    "root:starting:index:firebase": "mv functions/dist/apps/ditectrev-server/main.js functions/index.js"
